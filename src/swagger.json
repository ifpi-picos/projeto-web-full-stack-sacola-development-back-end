{
  "openapi": "3.0.0",
  "info": {
    "title": "API",
    "description": "API",
    "termsOfService": "https://swagger.io/terms/",
    "contact": {
      "name": "API Support",
      "url": "https://www.swagger.io/support",
      "email": "neoarmandosousa1010@hotmail.com"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:5000",
      "description": "Servidor de desenvolvimento"
    },
    {
      "url": "https://projeto-web-full-stack-sacola-development-back-end.vercel.app/",
      "description": "Servidor de produção"
    }
  ],
  "paths": {
    "/api/users": {
      "post": {
        "tags": [
          "Users"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "Client_Token",
            "in": "header",
            "required": true
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Usuário criado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "user": {
                      "$ref": "#/components/schemas/User"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Não Autorizado: A autenticação é necessária e falhou ou não foi fornecida.",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Mensagem de erro"
                    }
                  }
                }
              }
            }
          },
          "409": {
            "description": "Conflito: O usuário já existe",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Mensagem de erro"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Erro interno do servidor",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Mensagem de erro"
                    }
                  }
                }
              }
            }
          }
        },
        "summary": "Cria um usuário",
        "description": "Cria um usuário",
        "operationId": "createUser"
      }
    },
    "/api/user": {
      "get": {
        "tags": [
          "Users"
        ],
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "responses": {
          "200": {
            "description": "Usuário encontrado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "user": {
                      "$ref": "#/components/schemas/User"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Não Autorizado: A autenticação é necessária e falhou ou não foi fornecida.",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "message": {
                      "type": "string",
                      "description": "Mensagem de erro"
                    }
                  }
                }
              }
            }
          }
        },
        "summary": "Busca um usuário",
        "description": "Busca um usuário",
        "operationId": "getUser"
      }
    }
  },
  "components": {
    "schemas": {
      "User": {
        "properties": {
          "_id": {
            "type": "string",
            "description": "Id do usuário"
          },
          "name": {
            "type": "string",
            "description": "Nome do usuário"
          },
          "username": {
            "type": "string",
            "description": "apelido do usuário"
          },
          "email": {
            "type": "string",
            "description": "email do usuário"
          },
          "photo": {
            "type": "string",
            "description": "foto do usuário"
          },
          "userFriends": {
            "type": "object",
            "description": "amigos do usuário"
          },
          "userGames": {
            "type": "object",
            "description": "jogos do usuário"
          },
          "storeKeys": {
            "type": "object",
            "description": "chaves das lojas do usuário"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}


